{
  "name": "redux-proto",
  "description": "redux-proto",
  "version": "0.0.0",
  "private": true,
  "main": "src/server/server.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production RS_ENV=prd run-s clean build:{client,server,size}",
    "build-storybook": "build-storybook",
    "build:client": "webpack --verbose --colors --display-error-details --config src/webpack/prod.client.config.js",
    "build:csr": "cross-env DISABLE_SSR=1 npm run build",
    "build:link-css": "cross-env DISABLE_INLINE_CSS=1 npm run build",
    "build:offload": "cross-env ENABLE_OFFLOAD=1 npm run build",
    "build:server": "webpack --verbose --colors --display-error-details --config src/webpack/prod.server.config.js",
    "build:size": "webpack-bundle-size-analyzer build/client/stats.json",
    "check:secure": "nsp check",
    "clean": "rimraf build/*",
    "clean-start": "run-s clean build start:prod",
    "fix": "run-s fix:*",
    "fix:js": "eslint bin src --fix",
    "lint": "run-s lint:*",
    "lint:js": "eslint bin src",
    "lint:style": "stylelint src",
    "prod": "run-s clean build start:prod",
    "screenshot": "rimraf __screenshots__; storybook-chrome-screenshot -p 9001 -c .storybook/screenshot",
    "start": "npm run start:dev",
    "start:csr": "cross-env DISABLE_SSR=1 npm run start:dev",
    "start:dev": "cross-env NODE_ENV=development run-p start:dev:{server,agreed}",
    "start:dev:agreed": "agreed-server --path ./spec/agreed/agreed.js --port 3010",
    "start:dev:server": "cross-env DEBUG=express:*,app:* node bin/start-server.js",
    "start:offload": "cross-env ENABLE_OFFLOAD=1 npm run start:dev",
    "start:prod": "cross-env NODE_ENV=production run-p start:{prod:server,dev:agreed}",
    "start:prod:server": "node bin/start-server.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:agreed": "agreed-server --path ./spec/agreed/agreed.js --port 3020",
    "test:old": "run-p test:agreed test:wait-on -r --print-label",
    "test:wait-on": "wait-on tcp:3020 && cross-env NODE_ENV=test BABEL_ENV=test:unit eater"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.23.0",
    "babel-traverse": "^6.26.0",
    "body-parser": "^1.17.1",
    "compression": "^1.7.0",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "date-fns": "^1.28.5",
    "debug": "^3.1.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-session": "^1.15.5",
    "favalid": "^0.5.0",
    "fetchr": "^0.5.37",
    "fumble": "^0.1.3",
    "hoist-non-react-statics": "^2.2.1",
    "jsonwebtoken": "^8.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "ms": "^2.0.0",
    "multer": "^1.3.0",
    "prop-types": "^15.5.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.0.1",
    "react-redux": "^5.0.4",
    "react-redux-analytics": "^0.1.6",
    "react-redux-analytics-sitecatalyst": "0.0.5",
    "react-router": "3.2.0",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.2",
    "react-universal-component": "^2.3.2",
    "react-virtualized": "^9.9.0",
    "recompose": "0.27.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.0",
    "redux-async-loader": "^1.2.4",
    "redux-effects-fetchr": "^1.1.0",
    "redux-effects-fetchr-cache": "^1.0.1",
    "redux-effects-steps": "^1.0.1",
    "redux-effects-universal-cookie": "^1.0.1",
    "redux-form": "^7.3.0",
    "redux-page-scope": "^1.0.0",
    "reselect": "^3.0.1",
    "serve-favicon": "^2.5.0",
    "server-timing": "^2.0.0",
    "styled-components": "^3.2.6",
    "webpack-flush-chunks": "^1.1.18"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.2",
    "@storybook/addon-storyshots": "^3.4.2",
    "@storybook/addons": "^3.4.2",
    "@storybook/react": "^3.4.2",
    "agreed": "^3.1.0",
    "assert-stream": "^1.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-imports": "^1.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^2.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-flowtype": "^2.46.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.76.0",
    "form-data": "^2.3.1",
    "jest": "^23.3.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^3.2.1",
    "power-assert": "^1.6.0",
    "prettier": "^1.13.7",
    "react-test-renderer": "^16.4.1",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "rimraf": "^2.6.1",
    "seamless-immutable": "^7.1.2",
    "stats-webpack-plugin": "^0.6.1",
    "storybook-chrome-screenshot": "^1.1.0-alpha.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "wait-on": "^2.0.2",
    "webpack": "^4.15.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "greenkeeper": {
    "ignore": [
      "react-router",
      "webpack",
      "webpack-flush-chunks",
      "webpack-dev-middleware",
      "webpack-hot-middleware",
      "webpack-hot-server-middleware",
      "eslint-config-airbnb",
      "flow-bin"
    ]
  },
  "jest": {
    "testMatch": [
      "**/__tests__/*.test.js"
    ]
  }
}
